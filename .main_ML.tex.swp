\n \rfr{leftInverseOperator}(A_L^{-1}, (A)) \iff (\rfr{matrix}(A, (n, m))) \land (\rfr{rank}(A) = n < m) \land 
\m (A_L^{-1} A = I = ((A^T A)^{-1} A^T) A)
\n \rfr{rightInverseOperator}(A_R^{-1}, (A)) \iff (\rfr{matrix}(A, (n, m))) \land (\rfr{rank}(A) = m < n) \land 
\m (A A_R^{-1} = I = A (A^T (A A^T)^{-1}))
\end{align}
\end{tcolorbox}

\subsection{Functional analysis}
\begin{tcolorbox}[breakable, enhanced]
\begin{align}
 \dfn{denseMap}\ml(L, \ml(D, H, +, \cdot, \langle \$1, \$2 \rangle\mr)\mr) \iff \ml(D \subseteq H\mr) \land \ml(\rfr{linearOperator}\ml(L, \ml(D, +, \cdot, H, +, \cdot\mr)\mr)\mr) \land
